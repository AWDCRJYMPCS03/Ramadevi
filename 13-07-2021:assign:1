import java.util.*;
class Main
{
public static void main(String args[])
{
int i,j;
System.out.println("Enter a number ");
Scanner sc=new Scanner(System.in);
int n=sc.nextInt();
primeNos(n);
}
static void primeNos(int n)
System.out.println("Prime numbers between 1 to ("+n+") are");
{
for(j=1;j<=n;j++)
{
int count=0;
for(i=1;i<=n;i++)
{
  if(i%j==0)
  {
       count++;        
  }
}
if(count==2)
System.out.println(i+ "");
   }
}
}
--------------------
output:
n=10;
prime no: 2 3 5 7;
