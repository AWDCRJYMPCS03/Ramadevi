import java.util.*;
class Main
{
   public static void main(String args[])
   {
       int i;
      int arr[]={5,10,15,20,25};
      int sum=0;
      {
         for(int num:arr)
         {
           sum=sum+num;
           }
      System.out.println("sum of array elements is"+sum);
   }
}
}
---------------
output:
enter the elements:5 10 15 20 25
sum of array elements is 75
