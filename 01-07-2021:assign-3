import java.util.*;
public class Main
{
public static void main (String args[]) 
{
    int i,j,n;
    char ch='A';
    for(i=1;i<=5;i++)
    {
        for(j=1;j<=i;j++)
        {
    System.out.print(ch+" ");
    ch++;
        }
        System.out.print("\n");
    }
}
}
------------
output:
A 
B C 
D E F 
G H I J 
K L M N O
